<?xml version='1.0' encoding='utf-8'?>
<odoo>
	<data>
		<!-- Add you code here -->
		<!-- cy.product.size form view -->
		<record id="cy_product_size_view_form" model="ir.ui.view">
			<field name="name">cy.product.size.view.form</field>
			<field name="model">cy.product.size</field>
			<field name="arch" type="xml">
				<form string="商品尺寸">
					<sheet>
						<group>
							<!-- Add your fields here -->
							<field name="value" />
							<field name="code" />
							<field name="active" />
							<field name="sequence" />
							<field name="is_in_filter" />
							<field name="is_default_in_filter" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<!-- cy.product.size tree view -->
		<record id="cy_product_size_view_tree" model="ir.ui.view">
			<field name="name">商品尺寸</field>
			<field name="model">cy.product.size</field>
			<field name="arch" type="xml">
				<tree>
					<!-- Add your fields here -->
					<field name="id" />
					<field name="value" />
					<field name="code" />
					<field name="active" />
					<field name="sequence"/>
					<field name="is_in_filter" />
					<field name="is_default_in_filter" />
				</tree>
			</field>
		</record>

		<!-- cy.product.size action window -->
		<record id="cy_product_size_action" model="ir.actions.act_window">
			<field name="name">商品尺寸</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">cy.product.size</field>
			<field name="view_mode">tree,form</field>
		</record>
	</data>
</odoo>